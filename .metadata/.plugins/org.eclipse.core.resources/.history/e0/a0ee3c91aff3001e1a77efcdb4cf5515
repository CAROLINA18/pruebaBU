package com.BU.springboot.backend.apirest.DTO;


import lombok.Data;
import lombok.NoArgsConstructor;

@Data
public class ProductoDTO {
	
	public ProductoDTO(Long id, String nombre, Double precio, java.util.Date date) {
		this.id = id;
		this.nombre = nombre;
		this.precio = precio;
		this.createAt = date;
	}
	private Long id;
    private String nombre;
    private Double precio;
    private Date createAt;
}